@prefix lmi: <http://github.com/EIS-Bonn/Luzzu#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

lmi:LuzzuMetricJavaImplementation a rdf:Class .

lmi:javaPackageName a rdf:Property ;
	rdfs:domain lmi:LuzzuMetricJavaImplementation ;
	rdfs:range xsd:String .
	
lmi:MetricConfiguration a rdf:Class .

lmi:metric a rdf:Property ;
	rdfs:domain lmi:MetricConfiguration ;
	rdfs:range xsd:String . 